{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host 0.0.0.0",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "openapi:convert": "swagger2openapi ../docs/swagger/gonuxt.swagger.json -o ../docs/swagger/gonuxt.openapi.json ",
