    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.6",
    "@nuxt/test-utils": "^3.18.0",
    "@nuxt/ui": "^3.1.1",
    "@pinia/nuxt": "^0.11.0",
    "@tailwindcss/vite": "^4.1.6",
    "@unhead/vue": "^2.0.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.26.0",
